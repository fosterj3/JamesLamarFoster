&lt;p&gt;In this project, I made two timelines with the same data using two different timline packages - timevis and vistime&lt;/p&gt;
&lt;pre class=&#34;r&#34;&gt;&lt;code&gt;library(tidyverse)
library(timevis)
library(vistime)&lt;/code&gt;&lt;/pre&gt;
&lt;pre class=&#34;r&#34;&gt;&lt;code&gt;#Create Dataframe of timeline

Date &amp;lt;-  as.Date(c(&amp;quot;2020-02-26&amp;quot;, &amp;quot;2020-02-28&amp;quot;, &amp;quot;2020-03-03&amp;quot;, &amp;quot;2020-03-06&amp;quot;, &amp;quot;2020-03-13&amp;quot;, &amp;quot;2020-03-17&amp;quot;, &amp;quot;2020-03-18&amp;quot;, &amp;quot;2020-03-20&amp;quot;, &amp;quot;2020-03-23&amp;quot;, &amp;quot;2020-03-23&amp;quot;, &amp;quot;2020-04-06&amp;quot;, &amp;quot;2020-04-07&amp;quot;, &amp;quot;2020-04-08&amp;quot;, &amp;quot;2020-04-15&amp;quot;, &amp;quot;2020-04-21&amp;quot;, &amp;quot;2020-04-27&amp;quot;, &amp;quot;2020-03-03&amp;quot;))

Source &amp;lt;- c(&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;,&amp;quot;OSPI&amp;quot;, &amp;#39;Northshore School District&amp;#39;)

Title &amp;lt;- c(&amp;#39;Letter to Superintendents&amp;#39;, 
          &amp;#39;Preparation for COVID-19 Outbreak&amp;#39;, 
          &amp;#39;Further COVID-19 Guidance&amp;#39;, 
          &amp;#39;Online Learning Guidance&amp;#39;,
          &amp;#39;Guidance for Long-term School Closures #1&amp;#39;, 
          &amp;#39;Superintendents and Labor Leaders Letter&amp;#39;, 
          &amp;#39;Guidance LSC #2&amp;#39;, 
          &amp;#39;Guidance LSC #3&amp;#39;, 
          &amp;#39;Guidance LSC #4&amp;#39;, 
          &amp;#39;Guidance LSC #5&amp;#39;, 
          &amp;#39;Continuous Learning 2020&amp;#39;, 
          &amp;#39;Inclusionary Practices Doc&amp;#39;, 
          &amp;#39;Guidance LSC #6&amp;#39;, 
          &amp;#39;Guidance LSC #7&amp;#39;, 
          &amp;#39;Student Learning &amp;amp; Grading Guidance&amp;#39;, 
          &amp;#39;Supporting English Learners&amp;#39;,
          &amp;#39;Staff remote learning PD&amp;#39;)

df &amp;lt;- data.frame(Date, Source, Title)

df1 &amp;lt;- df %&amp;gt;% 
  arrange(Date)&lt;/code&gt;&lt;/pre&gt;
&lt;pre class=&#34;r&#34;&gt;&lt;code&gt;#Create timeline with timevis
df2 &amp;lt;- data.frame(
  id = 1:17,
  content = c(&amp;#39;Letter to Superintendents&amp;#39;, 
          &amp;#39;Preparation for COVID-19 Outbreak&amp;#39;, 
          &amp;#39;Further COVID-19 Guidance&amp;#39;, 
          &amp;#39;Online Learning Guidance&amp;#39;,
          &amp;#39;Guidance for Long-term School Closures #1&amp;#39;, 
          &amp;#39;Superintendents and Labor Leaders Letter&amp;#39;, 
          &amp;#39;Guidance LSC #2&amp;#39;, 
          &amp;#39;Guidance LSC #3&amp;#39;, 
          &amp;#39;Guidance LSC #4&amp;#39;, 
          &amp;#39;Guidance LSC #5&amp;#39;, 
          &amp;#39;Continuous Learning 2020&amp;#39;, 
          &amp;#39;Inclusionary Practices Doc&amp;#39;, 
          &amp;#39;Guidance LSC #6&amp;#39;, 
          &amp;#39;Guidance LSC #7&amp;#39;, 
          &amp;#39;Student Learning &amp;amp; Grading Guidance&amp;#39;, 
          &amp;#39;Supporting English Learners&amp;#39;,
          &amp;#39;Staff remote learning PD&amp;#39;),
  start = c(&amp;quot;2020-02-26&amp;quot;, 
            &amp;quot;2020-02-28&amp;quot;, 
            &amp;quot;2020-03-03&amp;quot;, 
            &amp;quot;2020-03-06&amp;quot;, 
            &amp;quot;2020-03-13&amp;quot;, 
            &amp;quot;2020-03-17&amp;quot;, 
            &amp;quot;2020-03-18&amp;quot;, 
            &amp;quot;2020-03-20&amp;quot;, 
            &amp;quot;2020-03-23&amp;quot;, 
            &amp;quot;2020-03-23&amp;quot;, 
            &amp;quot;2020-04-06&amp;quot;, 
            &amp;quot;2020-04-07&amp;quot;, 
            &amp;quot;2020-04-08&amp;quot;, 
            &amp;quot;2020-04-15&amp;quot;, 
            &amp;quot;2020-04-21&amp;quot;, 
            &amp;quot;2020-04-27&amp;quot;, 
            &amp;quot;2020-03-03&amp;quot;),
  end = c(NA),
  options = list(editable = T, height = &amp;quot;200px&amp;quot;)
)&lt;/code&gt;&lt;/pre&gt;
&lt;pre class=&#34;r&#34;&gt;&lt;code&gt;timevis(df2)&lt;/code&gt;&lt;/pre&gt;
